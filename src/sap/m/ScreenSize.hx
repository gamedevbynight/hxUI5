package sap.m;

@:native("sap.m.ScreenSize")
@:enum extern abstract ScreenSize(String)
{
    /**
    * 1024px wide
    */
    var Desktop= "Desktop";
    /**
    * 768px wide
    */
    var Large= "Large";
    /**
    * 560px wide
    */
    var Medium= "Medium";
    /**
    * 240px wide
    */
    var Phone= "Phone";
    /**
    * 480px wide
    */
    var Small= "Small";
    /**
    * 600px wide
    */
    var Tablet= "Tablet";
    /**
    * 960px wide
    */
    var XLarge= "XLarge";
    /**
    * 320px wide
    */
    var XSmall= "XSmall";
    /**
    * 1120px wide
    */
    var XXLarge= "XXLarge";
    /**
    * 240px wide
    */
    var XXSmall= "XXSmall";
}
