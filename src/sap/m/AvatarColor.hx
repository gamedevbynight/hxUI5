    */
    var Accent3= "Accent3";
    /**
    * Accent 4
    */
    var Accent4= "Accent4";
    /**
    * Accent 5
    */
    var Accent5= "Accent5";
    /**
    * Accent 6
    */
    var Accent6= "Accent6";
    /**
    * Accent 7
    */
    var Accent7= "Accent7";
    /**
    * Accent 8
    */
    var Accent8= "Accent8";
    /**
    * Accent 9
    */
    var Accent9= "Accent9";
    /**
    * Recommended when used as a placeholder (no image or initials are provided).
    */
    var Placeholder= "Placeholder";
    /**
    * Random color, chosen from the accent options (Accent1 to Accent10)
    */
    var Random= "Random";
